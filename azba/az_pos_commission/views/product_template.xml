<?xml version="1.0"?>
<odoo>

    <!-- Form View  -->
    <record id="az_product_commision_category" model="ir.ui.view">
        <field name="name">az.product.commission.category.form.inherit</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_only_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='pos_categ_id']" position="after">
                    <field name="commission_category"/>
            </xpath>
        </field>
    </record>


</odoo>
