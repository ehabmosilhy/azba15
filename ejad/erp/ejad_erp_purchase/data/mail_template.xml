<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <record id="nauss_mail_template" model="mail.template">
            <field name="name">وثايق منتهية الصلحية</field>
            <field name="subject">وثايق منتهية الصلحية</field>
            <field name="email_from">${(object.user_id.email and '&quot;%s&quot; &lt;%s&gt;' % (object.user_id.name, object.user_id.email) or '')|safe}</field>
            <field name="email_to">${object.email}</field>
            <field name="lang">${object.lang}</field>
            <field name="model_id" ref="ejad_erp_purchase.model_res_partner"/>
            <field name="auto_delete" eval="True"/>
            <field name="body_html" ><![CDATA[لديك بعض الوثايق الرسمية منتهية الصلوحية ..]]></field>
        </record>

</odoo>