<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_material_report" model="ir.ui.view">
        <field name="name">Material price Report</field>
        <field name="model">wiz.material.price</field>
        <field name="arch" type="xml">
            <form string="Choose your date">
                <separator string="فئة المواد"/>
                <group>
                    <field name="category_id"/>
                </group>
                <footer>
                    <button name="check_report" string="طباعة" type="object" class="btn-primary"/>
                    <button string="إلغاء" class="btn-default" special="cancel" />
                </footer>
            </form>
        </field>
    </record>
    <record id="action_material_report" model="ir.actions.act_window">
        <field name="name">تقرير تحليل الأسعار</field>
        <field name="res_model">wiz.material.price</field>
        <!--<field name="view_type">form</field>-->
        <field name="view_mode">form</field>
        <field name="view_id" ref="view_material_report"/>
        <field name="target">new</field>
    </record>
    <menuitem id="menu_material_price_report" name="تقرير تحليل الاسعار" parent="menu_nacsu_purchase_report" action="action_material_report"/>
</odoo>