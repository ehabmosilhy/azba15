<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="mwan_personal_id_res_partner">
            <field name="name">personal.id.res.partner</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='vat']" position="after">
                    <field name="tax_reg_no" attrs="{'invisible': [('company_type', '!=', 'company')]}"/>
                    <field name="commercial_reg_no" attrs="{'invisible': [('company_type', '!=', 'company')]}"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>