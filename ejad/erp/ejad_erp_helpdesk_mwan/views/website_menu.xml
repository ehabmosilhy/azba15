        <!--first page whem click menu (mwan contact us ) this template show 4 fields and choose-->
        <template id="contactus" name="Contact us">
            <t t-call="website.layout">
                <t t-set="pageName" t-value="'Mwan contact us Type'"/>
                <div id="wrap" class="oe_structure oe_empt">
                    <div class="container">
                        <div class="row justify-content-center">
                            <section class="helpdesk-forms col-md-8">
                                <div style="text-align: left;">
                                    <a id="query-ticket" t-attf-href="/helpdesk/query_ticket">
                                        <button t-attf-class="btn btn-primary">
                                            Query a Ticket
                                        </button>
                                    </a>
                                </div>
                                <div class="row">
                                    <form id="contactus_type" action="/contactus_type/submit" method="post"
                                          class="form s_website_form container-fluid mt32 o_fake_not_editable">
                                        <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()"/>
                                        <div class="form-group form-field">
                                            <h2>Type</h2>
                                            <select type="text"
                                                    class="custom-select form-control o_website_form_input"
                                                    id="ticket_type_id"
                                                    name="ticket_type_id" required="1">
                                                <option value=""></option>
                                                <t t-foreach="default_values['ticket_types']" t-as="type">
                                                    <option t-att-value="type.id">
                                                        <t t-esc="type.name"/>
                                                    </option>
                                                </t>
                                            </select>
                                        </div>
                                        <div class="form-group form-field" id="ticket_type_line_div"></div>
                                        <!-- selection company or person -->
                                        <div class="form-group form-field o_website_form_required">
                                            <h2>Entity Type</h2>
                                            <select type="text" class="form-control o_website_form_input"
                                                    id="entity_type"
                                                    name="entity_type">
                                                <option value="company">Company</option>
                                                <option value="person">Person</option>
                                                <option value="public_sector">Public Sector</option>
                                            </select>
                                        </div>

                                        <div class="form-group form-field d-none">
                                            <label for="team_id">Team</label>
                                            <input type="hidden" class="form-control o_website_form_input"
                                                   name="team_id" t-att-value="team.id"/>
                                        </div>
<!--                                        <input class="btn btn-success" type="submit" value="Submit"/>-->
                                        <button type="submit" class="btn btn-primary" value="Submit">Submit</button>

                                    </form>
                                </div> <!-- End Row -->
                            </section>
                        </div>
                    </div>
                </div>
            </t>
        </template>

        <!--second page which responsible for submit ticket-->
