<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_users_form" model="ir.ui.view">
        <field name="name">Adding Services for multi partners invoice</field>
        <field name="model">res.users</field>
        <field name="inherit_id" ref="base.view_users_form"/>
        <field name="arch" type="xml">
            <field name="lang" position="after">
                <field name="barcode_department"/>
                <field name="invoice_partner_ids" widget="many2many_tags"/>
            </field>
        </field>
    </record>

    <record model="ir.ui.view" id="partner_form_inherite">
        <field name="name">partner.form.inherit</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='company_type']" position="replace">

                <field name="company_type" options="{'horizontal': true}" class="oe_edit_only" widget="radio"
                      attrs="{'invisible': [('supplier_rank','=', 0)]}"/>
                <field name="company_type_nauss" options="{'horizontal': true}" class="oe_edit_only" widget="radio"
                       attrs="{'invisible': [('supplier_rank','!=', 0)]}"/>

            </xpath>
            <xpath expr="//field[@name='category_id']" position="before">
                <field name="customer_rank" invisible="1"/>
                <field name="supplier_rank" invisible="1"/>
                <field name="college" attrs="{'invisible': [('customer_rank','=', 0)]}"/>
                <field name="uni_id" attrs="{'invisible': [('customer_rank','=', 0)]}"/>
                <field name="identify_number"
                       attrs="{'invisible': [('customer_rank','=', 0),('supplier_rank','=', 0)]}"/>

                <field name="nomination_no" attrs="{'invisible': [('customer_rank','=', 0)]}"/>
                <field name="nomination_date" attrs="{'invisible': [('customer_rank','=', 0)]}"/>
                <field name="nomination_report_recipient" attrs="{'invisible': [('customer_rank','=', 0)]}"/>


                <field name="supplier_id" attrs="{'invisible': [('supplier_rank','=', 0)]}"/>
            </xpath>


        </field>
    </record>
