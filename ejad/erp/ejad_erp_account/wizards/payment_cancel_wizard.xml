<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record id="view_reverse_entry_wizard_form" model="ir.ui.view">
            <field name="name">reverse.entry.wizard.form</field>
            <field name="model">reverse.entry.wizard</field>
            <field name="arch" type="xml">
                <form string="Cancel Payment">
                    <!--<header>-->
                    <!--<button icon="fa-check-square-o" string="Validate" name="save" type="object"-->
                    <!--class="oe_highlight"/>-->
                    <!--</header>-->
                    <sheet>
                        <group>
                            <group>
                                <field name="date"/>

                            </group>
                            <group>
                                <field name="journal_id"/>

                            </group>

                        </group>
                        <group>
                            <field name="reason"/>


                        </group>
                    </sheet>
                    <footer>
                        <button string='Validate' name="action_validate_reserve_entry" type="object"
                                class="btn-primary"/>
                        <button string="Cancel" class="btn-default" special="cancel"/>
                    </footer>

                </form>

            </field>
        </record>

        <record id="action_reverse_entry_wizard" model="ir.actions.act_window">
            <field name="name">Cancel Payment</field>
            <field name="res_model">reverse.entry.wizard</field>
            <!--<field name="view_type">form</field>-->
            <field name="view_mode">form</field>
            <field name="view_id" ref="view_reverse_entry_wizard_form"/>
            <field name="target">new</field>
        </record>

    </data>
</openerp>