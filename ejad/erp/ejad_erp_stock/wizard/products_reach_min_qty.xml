<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_products_reach_min_qty_report" model="ir.ui.view">
        <field name="name">Product Reach Min Qty Report</field>
        <field name="model">wiz.product.min.qyt.report</field>
        <field name="arch" type="xml">
            <form string="Choose your date">
                <group>
                    <group>
                        <field name="date_from"/>
                    </group>
                    <group>
                        <field name="date_to"/>
                    </group>
                </group>
                <footer>
                    <button name="check_report" string="طباعة" type="object" class="btn-primary"/>
                    <button string="إلغاء" class="btn-default" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
    <record id="action_product_reach_min_qty" model="ir.actions.act_window">
        <field name="name">تقرير المنتجات وصلت حد الطلب</field>
        <field name="res_model">wiz.product.min.qyt.report</field>
        <!--<field name="view_type">form</field>-->
        <field name="view_mode">form</field>
        <field name="view_id" ref="view_products_reach_min_qty_report"/>
        <field name="target">new</field>
    </record>

    <menuitem id="menu_product_min_qty" name="المنتجات وصلت حد الطلب" action="action_product_reach_min_qty"
              parent="stock.menu_warehouse_report" sequence="200"/>
</odoo>

