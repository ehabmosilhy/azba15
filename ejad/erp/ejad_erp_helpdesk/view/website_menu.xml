<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <record id="user_logged_access_website_menu_tree" model="ir.ui.view">
            <field name="name">Website.menu.tree: Display to user who logged in</field>
            <field name="model">website.menu</field>
            <field name="inherit_id" ref="website.menu_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='new_window']" position="after">
                    <!--                <field name="is_megamenu"/>-->
                    <field name="user_logged"/>
                    <field name="user_not_logged"/>
                </xpath>
            </field>
        </record>

        <record id="user_logged_access_website_menu_form" model="ir.ui.view">
            <field name="name">Website.menu.form: Display to user who logged in</field>
            <field name="model">website.menu</field>
            <field name="inherit_id" ref="website.website_menus_form_view"/>
            <field name="arch" type="xml">
                <field name="new_window" position="after">
                    <field name="user_logged"/>
                    <field name="user_not_logged"/>
                </field>
            </field>
        </record>
    </data>
</odoo>