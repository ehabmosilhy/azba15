            <field name="name">employee.major.form</field>
            <field name="model">employee.major</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Employees majors">
                    <group>
                        <group>
                            <field name="name"/>

                        </group>
                    </group>
                </form>
            </field>
