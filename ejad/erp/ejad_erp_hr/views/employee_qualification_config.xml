            <field name="model">employee.qualification</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="Employees Qualifications">
                    <field name="name"/>
                </tree>
            </field>
        </record>
        <record id="view_employee_qualification_form" model="ir.ui.view">
            <field name="name">employee.qualification.form</field>
            <field name="model">employee.qualification</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Employees Qualifications">
                    <group>
                        <group>
                            <field name="name"/>

                        </group>
                    </group>
                </form>
            </field>
        </record>
        <record id="view_employee_major_tree" model="ir.ui.view">
            <field name="name">employee.major.tree</field>
            <field name="model">employee.major</field>
            <field name="type">tree</field>
