<odoo>
    <data noupdate="1">
       <record id="ir_cron_contract_alarm_action" model="ir.cron">
            <field name="name">HR Contract Alarm</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field eval="True" name="doall"/>
            <field ref="model_hr_contract_alarm" name="model_id"/>
            <field name="code">model.contract_check()</field>
            <field name="state">code</field>
        </record>


        <record id="seq_contract_ref" model="ir.sequence">
            <field name="name">Contract Reference</field>
            <field name="code">contract.ref</field>
            <field name="prefix">EC/%(year)s/</field>
            <field name="padding">5</field>
        </record>
	    <record id="ir_seq_hr_deduction" model="ir.sequence">
            <field name="name">deduction</field>
            <field name="code">hr.deduction.req</field>
            <field name="prefix">DED/%(year)s/ </field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="number_next_actual">1</field>
            <field name="implementation">standard</field>
        </record>
        <record id="ir_seq_hr_loan" model="ir.sequence">
            <field name="name">Loan Request</field>
            <field name="code">hr.loan.seq</field>
            <field name="prefix">LO/%(year)s/ </field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="number_next_actual">1</field>
            <field name="implementation">standard</field>
        </record>

        <record id="ir_cron_get_check_in_out_action" model="ir.cron">
            <field name="name">HR Employee Check In/Out</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field eval="True" name="doall"/>
            <field ref="model_hr_attendance" name="model_id"/>
            <field name="code">model.get_check_in_out_data()</field>
            <field name="state">code</field>
        </record>

   </data>
</odoo>
