<odoo>
    <record id="view__job_company_form_inherit_website" model="ir.ui.view">
        <field name="name">res.company.form.inherit.website</field>
        <field name="model">res.company</field>
        <field name="inherit_id"
               ref="base.view_company_form"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='social_media']" position="inside">
                <field name="social_skype"/>
                <field name="social_vimeo"/>
                <field name="social_rss"/>
                <field name="social_instagram"/>
            </xpath>
        </field>
    </record>
</odoo>
